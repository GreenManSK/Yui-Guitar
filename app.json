    "slug": "yui",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/tan_tan.gif",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
